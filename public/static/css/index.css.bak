body{
				width:100%;
				height:100%;
				margin:0px;
				font-family: 'Microsoft Yahei', Arial, Helvetica, "黑体", sans-serif;
			}
			#a{
				position:absolute;
				width:100%;
				height:100%;
				display: none;
				background-color: black;
				z-index: 100;
				margin:0px;
				opacity:0.7;
				}
			#mob_index{
				width:100%;
				height:100%;
			}
			
			#top_index{
				width:100%;
				height:10%;
			}
			#top_meau{
				width:8%;
				padding-top:5%;
				padding-left:3%;
				float:left;
			}
			#top_meau img{
				width:100%;
			}
			#top_logo{
				padding-top:2%;
				padding-left:20%;
				width:30%;
				float:left;
			}
			#top_email{
				padding-left:20%;
				padding-top:4%;
				width:15%;
				float:left;
			}
			#top_logo img{
				width:100%;
			}
			
			#top_email img{
				width:100%;
			}			
			#main_index{
				width:100%;
				height:85%;
			}
			#main_pic{
				width:90%;
				margin:auto;
				height:30%;
				border-radius: 7px;
				position: relative;
				overflow: hidden;
			}
			#main_pic .roll_ul{
				width:300%;
				height:100%;
				position: absolute;
				left: -12%;
    			top: -9.5%;
			}
			#main_pic li{
				width:33.333333%;
				list-style: none;
				float:left;
			}
			#main_pic li img{
				width:100%;
				height:100%;
			}
			
			#main_serach{
				width:100%;
				height:9%;
				margin-top:5%
			}
			#main_serach p{
			margin-left:4%;
			color:orange;
			/*font-family: "微软雅黑";*/
			}
			#search{
				width:90%;
				height:100%;
				margin: auto;
				background: no-repeat;
				border: 2px solid gainsboro;
				border-radius: 10px;
			}
			#index_sub{
				width:12%;
				border: none;
				margin-left:3%;
				/*background-color: white;*/
				border-radius: 0px 10px 10px 0px;
				vertical-align:middle;
			}
			#index_text{
				padding-bottom:0%;
				width:84%;
				height:50%;
				border: none;
				border-radius: 10px 0px 0px 10px;
				vertical-align:middle;
				font-size: 20px;			
			}
			input::-webkit-input-placeholder { /* WebKit browsers */ 
			font-size: 15px;
			} 
			input:-moz-placeholder { /* Mozilla Firefox 4 to 18 */ 
			font-size: 15px; 
			} 
			input::-moz-placeholder { /* Mozilla Firefox 19+ */ 
			font-size: 15px;
			} 
			input:-ms-input-placeholder { /* Internet Explorer 10+ */ 
			font-size: 15px;
			} 
			#main_meau{
				width:100%;
				height:60%;
				margin-top:10px;
			}
			.meau_list{
				width:45%;
				height:50%;
				float:left;
				margin-top:3%;
				margin-left:3%;
				border-radius: 7px;
			}
			.meau_list p{
				margin:0;
				padding:0;
				color: white;
				height:20%;
				/*font-family: "微软雅黑";*/
				margin-left:5%;
				line-height: 100%;
			}
			.meau_list img{
				width:80%;
				margin-left:13%;
			}
			.meau_list_list{
				width:45%;
				height:100%;
				float:left;
				border-radius: 7px;
			}
			
			
			#index_left{
				width:70%;
				height:100%;
				background-color: white;
				position: absolute;
				left:-70%;
				top:0%;
				z-index: 200;
			}
			#left_top{
				height:5%;
				padding-top: 20%;
				padding-bottom: 10%;
			}
			#left_top input{
				width:15%;
				padding-left:25%;
				vertical-align:middle;
			}
			#left_top img{
				margin-left: 5%;
				padding-right: 5%;
				width:20%;
				vertical-align:middle;
			}
			#left_top span{
				color: orange;
				font-size: 20px;
				vertical-align:middle;
			}
			#left_main{
				width:80%;
				height:80%;
			}
			#left_main ul{
				width:100%;
				height:100%;				
			}
			#left_main li{
				color: orange;
				font-size: 17px;
				list-style: none;
				line-height:300%;
			}
			


#bottom{
	width:100%;
	height:10%;
	position: fixed;
	top:90%;
	left:0%;
	background-color: #7EC0EE;
}
